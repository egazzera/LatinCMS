<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="LatinCMS" namespace="LatinCMS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Config" table="config" lazy="true" >
    <id name="Id" column="id">
      <generator class="identity" />
    </id>
    <property name="Clave">
      <column name="clave" sql-type="int" not-null="false" />
    </property>
    <property name="Valor">
      <column name="valor" sql-type="int" not-null="false" />
    </property>
  </class>
</hibernate-mapping>