<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="LatinCMS" namespace="LatinCMS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Comentario" table="comentario" lazy="true" >
    <id name="Id" type="int" column="id">
      <generator class="identity" />
    </id>
    <property name="Descripcion">
      <column name="descripcion" sql-type="nvarchar" not-null="false" />
    </property>
    <many-to-one name="Post" class="Post" foreign-key="Post" column="id" />
    <many-to-one name="Estadocomen" class="EstadoComen" foreign-key="Estadocomen" column="id" />
  </class>
</hibernate-mapping>