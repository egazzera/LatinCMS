<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="LatinCMS" namespace="LatinCMS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Post" table="post" lazy="true" >
    <id name="Id" column="id">
      <generator class="identity" />
    </id>
    <property name="Fecha">
      <column name="fecha" sql-type="smalldatetime" not-null="false" />
    </property>
    <property name="Descripcion">
      <column name="descripcion" sql-type="nvarchar" not-null="false" />
    </property>
    <many-to-one name="Usuario" class="Usuario" foreign-key="Usuario" column="id" />
    <many-to-one name="TipoPost" class="TipoPost" foreign-key="TipoPost" column="id" />
  </class>
</hibernate-mapping>