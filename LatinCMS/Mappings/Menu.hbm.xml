<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="LatinCMS" namespace="LatinCMS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Menu" table="menu" lazy="true" >
    <id name="Id" column="id">
      <generator class="identity" />
    </id>
    <property name="Titulo">
      <column name="titulo" sql-type="nvarchar" not-null="true" />
    </property>
    <many-to-one name="MenuVal" class="Menu" foreign-key="MenuVal" column="padre_id" />
    <many-to-one name="Post" class="Post" foreign-key="Post" column="id" />
  </class>
</hibernate-mapping>