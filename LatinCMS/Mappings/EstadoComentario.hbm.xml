<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="LatinCMS" namespace="LatinCMS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Estadocomentario" table="estadocomentario" lazy="true" >
    <id name="Id" column="id" />
    <property name="Descripcion">
      <column name="descripcion" sql-type="nchar" not-null="false" />
    </property>
    <bag name="Comentario">
      <key column="ec_id" />
      <one-to-many class="Comentario" />
    </bag>
  </class>
</hibernate-mapping>